AA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAzBA;AA0BA,IAAA,WA1BA,yBA0BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,WAAA,EADA;;AAAA;AAAA;AAAA,uBAGA,KAAA,8CACA,MAAA,CAAA,OAAA,CAAA,MADA,QAEA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,OAAA,EAAA;AACA,oCAAA;AADA,mBAFA;AAKA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,OAAA;AALA,iBAFA,CAHA;;AAAA;AAAA;AAAA,uBAcA,MAAA,CAAA,WAAA,EAdA;;AAAA;AAeA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KA1CA;AA2CA,IAAA,aA3CA,yBA2CA,OA3CA,EA2CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,WAAA,EADA;;AAAA;AAAA;AAAA,uBAGA,KAAA,8CAAA,OAAA,CAAA,MAAA,QAAA;AACA,kBAAA,MAAA,EAAA,QADA;AAEA,kBAAA,OAAA,EAAA;AACA,oCAAA;AADA,mBAFA;AAKA,kBAAA,IAAA,EAAA,IAAA,CAAA,SAAA,CAAA,MAAA,CAAA,OAAA;AALA,iBAAA,CAHA;;AAAA;AAAA;AAAA,uBAWA,MAAA,CAAA,WAAA,EAXA;;AAAA;AAYA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA;AAxDA;AAXA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <form @submit.prevent=\"submitForm\">\n      <div class=\"form-group column\">\n        <input\n          type=\"text\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"รหัสนิสิต\"\n          v-model=\"student.userid\"\n        />\n        <input\n          type=\"text\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"ชื่อ\"\n          v-model=\"student.name\"\n        />\n        <input\n          type=\"text\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"นามสกุล\"\n          v-model=\"student.surname\"\n        />\n        <input\n          type=\"text\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"เลขบัตรประชาชน\"\n          v-model=\"student.idcard\"\n        />\n        <input\n          type=\"date\"\n          min=\"2020-12-30\"\n          max=\"2200-12-31\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"วันจบการศึกษา\"\n          v-model=\"student.commencementday\"\n        />\n        <input\n          type=\"text\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"อีเมล\"\n          v-model=\"student.email\"\n        />\n        <input\n          type=\"text\"\n          class=\"form-control col-3 mx-2\"\n          placeholder=\"เบอร์โทรศัพท์\"\n          v-model=\"student.telphoneNumber\"\n        />\n        <button class=\"btn btn-success\"></button>\n      </div>\n    </form>\n    <table class=\"table\">\n      <thead>\n        <th>รหัสนิสิต</th>\n        <th>ชื่อ</th>\n        <th>นามสกุล</th>\n        <th>เลขบัตรประชาชน</th>\n        <th>วันจบการศึกษา</th>\n        <th>อีเมล</th>\n        <th>เบอร์โทรศัพท์</th>\n      </thead>\n      <tbody>\n        <tr\n          v-for=\"student in students\"\n          :key=\"student.userid\"\n          @dblclick=\"$data.student = student\"\n        >\n          <td>{{ student.userid }}</td>\n          <td>{{ student.name }}</td>\n          <td>{{ student.surname }}</td>\n          <td>{{ student.idcard }}</td>\n          <td>{{ student.commencementday }}</td>\n          <td>{{ student.email }}</td>\n          <td>{{ student.telphoneNumber }}</td>\n          <td>\n            <button\n              class=\"btn btn-outline-danger btn-sm mx-1\"\n              @click=\"deleteStudent(student)\"\n            >\n              X\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      student: {},\n      students: [],\n    };\n  },\n  async created() {\n    await this.getStudents();\n  },\n  methods: {\n    submitForm() {\n      if (this.student.id === undefined) {\n        this.createStudent();\n      } else {\n        this.editStudent();\n      }\n    },\n    async getStudents() {\n      var response = await fetch(\"http://localhost:8000/api/students/\");\n      this.students = await response.json();\n    },\n    async createStudent() {\n      await this.getStudents();\n\n      await fetch(\"http://localhost:8000/api/students/\", {\n        method: \"post\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n        body: JSON.stringify(this.student),\n      });\n\n      await this.getStudents();\n      this.student = {};\n    },\n    async editStudent() {\n      await this.getStudents();\n\n      await fetch(\n        `http://localhost:8000/api/students/${this.student.userid}/`,\n        {\n          method: \"put\",\n          headers: {\n            \"Content-Type\": \"application/json\",\n          },\n          body: JSON.stringify(this.student),\n        }\n      );\n\n      await this.getStudents();\n      this.student = {};\n    },\n    async deleteStudent(student) {\n      await this.getStudents();\n\n      await fetch(`http://localhost:8000/api/students/${student.userid}/`, {\n        method: \"delete\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n        body: JSON.stringify(this.student),\n      });\n\n      await this.getStudents();\n      this.student = {};\n    },\n  },\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"sourceRoot":"src"}]}