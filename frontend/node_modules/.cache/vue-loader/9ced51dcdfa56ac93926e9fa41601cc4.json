      <td>{{ student.idcard }}</td>\r\n          <td>{{ student.commencementday }}</td>\r\n          <td>{{ student.email }}</td>\r\n          <td>{{ student.telphoneNumber }}</td>\r\n          <td>\r\n            <button\r\n              class=\"btn btn-outline-danger btn-sm mx-1\"\r\n              @click=\"deleteStudent(student)\"\r\n            >\r\n              X\r\n            </button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <div class=\"container d-flex justify-content-center card\">\r\n      <h2>Register</h2>\r\n\r\n      <form @submit.prevent=\"submitForm\">\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Student Code</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"รหัสนิสิต\"\r\n              v-model=\"student.userid\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Year</label>\r\n            <select class=\"form-control\" v-model=\"student.year\" required>\r\n              <option disabled value=\"\">ชั้นปี</option>\r\n              <option>1</option>\r\n              <option>2</option>\r\n              <option>3</option>\r\n              <option>4</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Passwords</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"รหัสผ่าน\"\r\n              v-model=\"student.password\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Name</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"ชื่อจริง\"\r\n              v-model=\"student.name\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Surname</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"นามสกุล\"\r\n              v-model=\"student.surname\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Id card</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"เลขบัตรประชาชน\"\r\n              v-model=\"student.idcard\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Email</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"อีเมล\"\r\n              v-model=\"student.email\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group\">\r\n          <div class=\"form-row col-md-12\">\r\n            <label>Telephone Number</label>\r\n            <input\r\n              type=\"text\"\r\n              class=\"form-control\"\r\n              placeholder=\"เบอร์โทรศัพท์\"\r\n              v-model=\"student.telphoneNumber\"\r\n              required\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <button class=\"btn btn-success\">Submit</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Registration\",\r\n  data() {\r\n    return {\r\n      student: {},\r\n      students: [],\r\n    };\r\n  },\r\n  async created() {\r\n    await this.getStudents();\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      if (this.student.userid !== undefined) {\r\n        this.createStudent();\r\n      } else {\r\n        //alert(\"This Student Code already exists\");\r\n        //this.editStudent();\r\n      }\r\n    },\r\n    async getStudents() {\r\n      var response = await fetch(\"http://localhost:8000/api/students/\");\r\n      this.students = await response.json();\r\n    },\r\n    async createStudent() {\r\n      await this.getStudents();\r\n\r\n      await fetch(\"http://localhost:8000/api/students/\", {\r\n        method: \"post\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify(this.student),\r\n      });\r\n      alert(\"Registration Completed success\");\r\n      await this.getStudents();\r\n      this.student = {};\r\n    },\r\n    async editStudent() {\r\n      await this.getStudents();\r\n\r\n      await fetch(\r\n        `http://localhost:8000/api/students/${this.student.userid}/`,\r\n        {\r\n          method: \"save\",\r\n          headers: {\r\n            \"Content-Type\": \"application/json\",\r\n          },\r\n          body: JSON.stringify(this.student),\r\n        }\r\n      );\r\n\r\n      await this.getStudents();\r\n      this.student = {};\r\n    },\r\n    async deleteStudent(student) {\r\n      await this.getStudents();\r\n\r\n      await fetch(`http://localhost:8000/api/students/${student.userid}/`, {\r\n        method: \"delete\",\r\n        headers: {\r\n          \"Content-Type\": \"application/json\",\r\n        },\r\n        body: JSON.stringify(this.student),\r\n      });\r\n\r\n      await this.getStudents();\r\n      this.student = {};\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n}\r\n</style>"]}]}